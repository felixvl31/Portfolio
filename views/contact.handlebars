{{> nav}}
<main class="main">
	<div class="container">
		<div class="row center">
			<div class="col s12">
				<h4>Contact Me</h4>
			</div>
		</div>
		<div class="row center">
			<div class="col s12 m6">
				<a class="contact-mine" href="mailto:felix.evl@gmail.com?subject=Contact from your site"><span
						class="contact-First">Email: </span><span class="contact-Second">felix.evl@gmail.com</span></a>
			</div>
			<div class="col s12 m6">
				<a class="contact-mine"><span class="contact-First">Phone:</span><span class="contact-Second">(512)
						586-7336</span></a>
			</div>
		</div>

		<form class="contact">
			<fieldset class="contact-info">
				<label>
					Name
					<input class="name" type="text" name="name" placeholder="John Doe" required>
				</label>
				<label>
					Email
					<input class="email" type="email" name="email" placeholder="mail@domain.com" required>
				</label>
				<label>
					Comments
					<textarea class="comments" name="comments" placeholder="Provide as much detail as possible"></textarea>
				</label>
			</fieldset>
			<div class="center">
				<input class="input-default" type="submit" name="submit" value="Submit">
			</div>
		</form>

</main>

{{> footer}}